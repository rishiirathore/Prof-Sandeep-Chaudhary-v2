    // Run this script immediately, before DOM is fully loaded
    (function () {
      // Apply theme immediately
      const savedTheme = localStorage.getItem("theme") || "dark";
      document.documentElement.setAttribute("data-theme", savedTheme);

      // Pre-set the checkbox state in the HTML
      if (savedTheme === "dark") {
        document.write(
          "<style>#theme-toggle{opacity:0}#theme-toggle.ready{opacity:1}</style>"
        );
        // This will run after the DOM is loaded to check the checkbox
        document.addEventListener("DOMContentLoaded", function () {
          document.getElementById("theme-toggle").checked = true;
        });
      }
    })();
  </script>


